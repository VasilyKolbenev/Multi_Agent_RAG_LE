{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/main.py"
    }
  ],
  "env": {
    "LLM_MODEL": "gpt-5-mini",
    "EMBEDDING_MODEL": "text-embedding-3-small"
  },
  "functions": {
    "api/main.py": {
      "maxDuration": 30
    }
  }
}
